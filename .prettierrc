{
	"arrowParens": "always",
	"endOfLine": "lf",
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true,
	"printWidth": 200,
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"useTabs": false
			}
		}
	]
}
